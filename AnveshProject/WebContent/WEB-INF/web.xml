<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>AnveshProject</display-name>
  <welcome-file-list>
    <welcome-file>firstpage.jsp</welcome-file>
  </welcome-file-list>
  
  <servlet>
    <servlet-name>fps</servlet-name>
    <servlet-class>com.bharath.selectpage.servlet.FirstPageServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>fps</servlet-name>
    <url-pattern>/fps</url-pattern>
  </servlet-mapping>
  
   <servlet>
    <servlet-name>sp</servlet-name>
    <servlet-class>com.bharath.selectpage.servlet.SecondPageServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>sp</servlet-name>
    <url-pattern>/sp</url-pattern>
  </servlet-mapping>
</web-app>